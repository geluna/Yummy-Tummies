<% content_for :title do %>My Profile<% end %>
<div class="page-header">
<h1 calss >Welcome to your profile!</h1>
</div>
</br>
<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.address %></td>
        <td><%= user.latitude %></td>
        <td><%= user.longitude %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>