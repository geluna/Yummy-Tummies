
<!DOCTYPE html>


	<html>
		<head>
  			<%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
 			<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
 			<%= csrf_meta_tags %>
 			
 		</head>

		<body>
			
			<div class="navbar transparent navbar-inverse navbar-fixed-top">
			<nav class="navbar navbar-inner">		
		<div class="container-fluid">
    		<div class="navbar-header">
      			<a class="navbar-brand" href="/store/index" style="margin-left: 120px;" ><%= t('.Yummy Tummies') %></a>
    		</div>
    		<div>
				<ul class="nav navbar-nav">		
					<li><%= link_to 'Menu', menus_url %></li>
			        <li><%= link_to 'My Cart', carts_path %></li>
			        <li><%= link_to 'My Orders', orders_path %></li>
			        <li><%= link_to 'About', page_path('about') %></li>
		  	    </ul>
				<ul class="nav navbar-nav navbar-right" style="margin-right: 20px">
						<% if user_signed_in? %>
						<li>
							<button type="button" class="btn btn-link" style="margin-top: 9px;">
							<%= link_to "logout", destroy_user_session_path, method: :delete %>
							</button>
						</li>
						<li>&nbsp;&nbsp;</li>
						<% else %>
						<li><%= render 'devise/registrations/form' %></li>
						<li>&nbsp;&nbsp;</li>
						<li><%= render 'devise/sessions/new2' %> </li>
						<li>&nbsp;&nbsp;</li>
						
						<% end %>
						<li>
						<div class="dropdown" style="margin-top: 8px;">
							<button class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-expanded="true">
								My Account
								<span class="caret"></span>
							</button>
							<ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
								<li><%= link_to 'Menu', menus_url %></li>
								<!-- <li><a href="#">My profile</a></li> -->

								<li><%= link_to 'My profile', page_path('../user/index'), 'data-no-turbolink' => true %></li>
								
								<li><a href="#">Payment History</a></li>
								<li class="divider"></li>
							</ul>
						</div>
						</li>
					<li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						&nbsp;&nbsp;</li>
			     </ul>
		    </div>
		 </div>
	</nav>

	</div>

<!--
	<div id="header">
		<img src="/assets/banner2.jpg" class="img-responsive center-block">
	</div>
		
 -->
	<div id="columns">
		<div id="main">
			<%= yield %>
		</div>
 	</div>
	</body>
</html>
 